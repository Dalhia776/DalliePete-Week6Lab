
  <div class="row">
        <div class="col-xs-3"><div class="well">
          <%= render :partial => "users/profile" %>
          </div>
          </div>
        <div class="col-xs-6"><div class="well">

            <%= render :partial => "posts/form" %>

          </div>
          </div>
        <div class="col-xs-3"><div class="well">
          <%= render :partial => "posts/suggFollow" %>
          </div>
          </div>
    </div>
    <div class="row">
        <div class="col-xs-3">
          <div class="well">
            <p>trends</p>
          </div>
        </div>
        <div class="col-xs-6">
          <div class="well">
              <h1>Whoots</h1>
                <% @posts.each do |post| %>
                  <div class="whootWrapper">
                    <p> <%= post.whoot %> </p>
                    <a href="<%= user_path post.user %>">
                    </a>
                      @<%= post.user.username %>
                      <span class="time"><%= time_ago_in_words(post.created_at) %></span>
                      <ul>
                        <li><%= link_to 'Edit', edit_post_path(post) %></li>
                        <li><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></li>
                      </ul>
                    </div>
                <% end %>
            </div>
          </div>

          <div class="col-xs-3">
            <div class="well">
              <p>3 xs cols</p>
            </div>
          </div>
        </div>
